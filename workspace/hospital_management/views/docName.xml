<odoo>
	<record id="view_doctor_form" model="ir.ui.view">
		<field name="name">doctor.name.form</field>
		<field name="model">doctor.name</field>
		<field name="arch" type="xml">
			<form>
				<header>
					<button type="object" name="action_count_degrees" class="oe_highlight"  string="countDegree"/>
				</header>
				<sheet>
					<div class="oe_button_box" name="button_box" >
		                <button name="action_open_patients" type="object" icon="fa-sitemap" class="oe_stat_button">
		                	<div class="o_stat_value">
		                    	<field string="count" name="no_of_apps" widget="statinfo"/>
		                    	<span class="o_stat_text">Appointments</span>
		                	</div>
		                </button>
		            </div>
					<group >    
						<group >
							<field name="name" attrs="{'readonly':[('name','=','Dr. Pinkal')]}"/>
							<field name="degrees_ids"  attrs="{'readonly':[('name','=','Dr. Pinkal')]}"/>
						</group>
						<group>
						     <field name="belongsTo" attrs="{'readonly':[('name','=','Dr. Pinkal')]}"/>
					    </group>
					</group>    
				</sheet>
			</form>
		</field>
	</record>

	<record id="view_doctor_tree" model="ir.ui.view">
		<field name="name">doctor.name.tree</field>
		<field name="model">doctor.name</field>
		<field name="arch" type="xml">
		   <tree>		   	
			  <field name="name"></field>
	          <field name="belongsTo"></field>
	          <field name="no_of_apps"></field>

		   </tree>
		</field>
	</record>
</odoo>    